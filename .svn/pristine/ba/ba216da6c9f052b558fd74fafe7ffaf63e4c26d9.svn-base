<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qst.grade.po.Comment" table="`comment`" schema="hand_to_hand">
        <id name="commentId" column="comment_id"/>
        <property name="commentator" column="commentator"/>
        <property name="respondent" column="respondent"/>
        <property name="gid" column="gid"/>
        <property name="content" column="content"/>
        <property name="status" column="status"/>
        <property name="createTime" column="create_time"/>
    </class>
</hibernate-mapping>