package com.qst.grade.po;

import javax.persistence.*;
import java.math.BigDecimal;

@Entity
@Table(name = "`goods`")
public class Goods {
    private String gid;
    private String name;
    private String detail;
    private int status;
    private String uid;
    private int count;
    private BigDecimal price;
    private String imgPath;
    private String acronym;
    private String fullpinyin;
    private String cid;

    @Id
    @Column(name = "gid")
    public String getGid() {
        return gid;
    }

    public void setGid(String gid) {
        this.gid = gid;
    }

    @Basic
    @Column(name = "name")
    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    @Basic
    @Column(name = "detail")
    public String getDetail() {
        return detail;
    }

    public void setDetail(String detail) {
        this.detail = detail;
    }

    @Basic
    @Column(name = "status")
    public int getStatus() {
        return status;
    }

    public void setStatus(int status) {
        this.status = status;
    }

    @Basic
    @Column(name = "uid")
    public String getUid() {
        return uid;
    }

    public void setUid(String uid) {
        this.uid = uid;
    }

    @Basic
    @Column(name = "count")
    public int getCount() {
        return count;
    }

    public void setCount(int count) {
        this.count = count;
    }

    @Basic
    @Column(name = "price")
    public BigDecimal getPrice() {
        return price;
    }

    public void setPrice(BigDecimal price) {
        this.price = price;
    }

    @Basic
    @Column(name = "img_path")
    public String getImgPath() {
        return imgPath;
    }

    public void setImgPath(String imgPath) {
        this.imgPath = imgPath;
    }

    @Basic
    @Column(name = "acronym")
    public String getAcronym() {
        return acronym;
    }

    public void setAcronym(String acronym) {
        this.acronym = acronym;
    }

    @Basic
    @Column(name = "fullpinyin")
    public String getFullpinyin() {
        return fullpinyin;
    }

    public void setFullpinyin(String fullpinyin) {
        this.fullpinyin = fullpinyin;
    }

    @Basic
    @Column(name = "cid")
    public String getCid() {
        return cid;
    }

    public void setCid(String cid) {
        this.cid = cid;
    }

    @Override
    public boolean equals(Object o) {
        if (this == o) return true;
        if (o == null || getClass() != o.getClass()) return false;

        Goods goods = (Goods) o;

        if (status != goods.status) return false;
        if (count != goods.count) return false;
        if (gid != null ? !gid.equals(goods.gid) : goods.gid != null) return false;
        if (name != null ? !name.equals(goods.name) : goods.name != null) return false;
        if (detail != null ? !detail.equals(goods.detail) : goods.detail != null) return false;
        if (uid != null ? !uid.equals(goods.uid) : goods.uid != null) return false;
        if (price != null ? !price.equals(goods.price) : goods.price != null) return false;
        if (imgPath != null ? !imgPath.equals(goods.imgPath) : goods.imgPath != null) return false;
        if (acronym != null ? !acronym.equals(goods.acronym) : goods.acronym != null) return false;
        if (fullpinyin != null ? !fullpinyin.equals(goods.fullpinyin) : goods.fullpinyin != null) return false;

        return true;
    }

    @Override
    public int hashCode() {
        int result = gid != null ? gid.hashCode() : 0;
        result = 31 * result + (name != null ? name.hashCode() : 0);
        result = 31 * result + (detail != null ? detail.hashCode() : 0);
        result = 31 * result + status;
        result = 31 * result + (uid != null ? uid.hashCode() : 0);
        result = 31 * result + count;
        result = 31 * result + (price != null ? price.hashCode() : 0);
        result = 31 * result + (imgPath != null ? imgPath.hashCode() : 0);
        result = 31 * result + (acronym != null ? acronym.hashCode() : 0);
        result = 31 * result + (fullpinyin != null ? fullpinyin.hashCode() : 0);
        return result;
    }

    @Override
    public String toString() {
        return "Goods{" +
                "gid='" + gid + '\'' +
                ", name='" + name + '\'' +
                ", detail='" + detail + '\'' +
                ", status=" + status +
                ", uid='" + uid + '\'' +
                ", count=" + count +
                ", price=" + price +
                ", imgPath='" + imgPath + '\'' +
                ", acronym='" + acronym + '\'' +
                ", fullpinyin='" + fullpinyin + '\'' +
                '}';
    }
}
