h3 {
	font-weight: 200;
	font-family: 'Open Sans', sans-serif;
	color: red !important;
}

.agileits_main {
	background: url(../images/b1.jpg);
	width: 100%;
	background-repeat: no-repeat;
	background-position: center;
	background-size: cover;
	height: 240px;
	text-align: left;
	padding: 30px 0px 30px 40px;
	color: white;
	position: absolute;
}

.agileits_main span {
	font-size: 45px;
	font-weight: 200;
}

.myimg {
	text-align: center;
	padding-bottom: 40px;
}

img {
	height: 150px;
	width: 150px;
	background-repeat: no-repeat;
	background-position: center;
	box-shadow: 0 0 10px black;

}


.xian {
	height: 1px;
	width: 300px;
	background-color: whitesmoke;

}

.info-body {
	height: auto;
	width: 100%;
	padding-top: 50px;
	position: relative;
	top: 110px;

}

.info {
	height: auto;
	*background-color: #D2D2D2;

}

.info-left {
	padding-left: 30px;
	text-align: right;
}

p {
	height: 50px;
	font-size: 17px;
	line-height: 50px;
}

.submit {
	text-align: center;
	padding-top: 30px;
	padding-bottom: 40px;
}

label {
	height: 50px;
	line-height: 50px;
	font-size: 17px;
	color: #000000;

}

a.update {
	color: #000000;
	text-decoration: none;
!important;
}

a.update:hover,
a.update:link {
	text-decoration: none;
	color: #919191;

}
a.update:visited{
	text-decoration: none;
	color: black;
}

button {
	height: 40px;
	width: 100px;
	line-height: 40px;
	border: 1px solid black;
	-webkit-transition-duration: 0.4s;
	/* Safari */
	transition-duration: 0.4s;
}

button:hover {
	background-color: black;
	color: white;
}
input{
	height: 40px;
	width: 80%;
	line-height: 40px;
	border: 1px solid #BBBBBB;
	background-color: white;
	margin: 10px 0px 10px 0px !important;
	
}
input:hover,input:link{
	border: 1px solid black;
}
.modal-title{
	color: black;
	font-size: 20px;
}
.modal-body{
	color: #000000;
	font-size: 15px;
}
#errorPass{
text-align: center;
	font-size: 16px;
}
.error{
text-align: center;
	color: red;
	padding-top: 10px;
}
a.return,a.return:visited{
	text-decoration: none;
	color: #F0F0F0;
}
a.return:hover,a.return:focus{
	color: #BABABA;
}
