<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.qst.grade.po.Address" table="`address`" schema="hand_to_hand">
        <id name="aid" column="aid"/>
        <property name="province" column="province"/>
        <property name="city" column="city"/>
        <property name="town" column="town"/>
        <property name="detailAddress" column="detail_address"/>
        <property name="telphone" column="telphone"/>
        <property name="uid" column="uid"/>
        <property name="nickname" column="nickname"/>
    </class>
</hibernate-mapping>